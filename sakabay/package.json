    "jest": "^26.4.2",
    "jquery": "^3.5.1",
    "jshint": "^2.12.0",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.19",
    "moment": "^2.29.1",
    "nouislider": "^14.6.0",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "swiftmailer-bundle": "symfony/swiftmailer-bundle",
    "vee-validate": "^2.1.0-beta.11",
    "vue": "^2.6.11",
    "vue-avatar": "^2.3.0",
    "vue-axios": "^2.1.5",
    "vue-i18n": "^8.21.0",
    "vue-lazyload": "^1.3.3",
    "vue-loader": "^15.9.3",
    "vue-loaders": "^4.0.0",
    "vue-multiselect": "^2.1.6",
    "vue-nl2br": "^0.1.2",
    "vue-observe-visibility": "^1.0.0",
