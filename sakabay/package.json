    "vue-test-utils": "^1.0.0-beta.11",
    "vue2-autocomplete-js": "^0.2.2",
    "vue2-editor": "^2.10.2",
    "vue2-leaflet": "^2.6.0",
    "vue2-transitions": "^0.3.0",
    "vuejs-logger": "^1.5.4",
    "vuescroll": "^4.17.3",
    "vuetify": "^2.3.9",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@symfony/webpack-encore": "^0.30.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.8",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-vue": "^7.4.0",
    "jest-vue-preprocessor": "^1.7.1",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.2",
    "sass-loader": "^8.0.0",
    "typescript": "^4.1.3",
    "typescript-eslint-parser": "^22.0.0",
    "webpack-notifier": "^1.6.0"
  },
  "license": "UNLICENSED",
  "private": true,
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "modules": "commonjs",
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest/__mocks__/fileMock.js",
      "^vue$": "vue/dist/vue.common.js"
    },
    "testRegex": "(/__tests__/.*|\\.tests?)\\.js$",
    "modulePaths": [
      "assets/js"
    ],
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.vue$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "coverageDirectory": "<rootDir>/tests/jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/mixins/*Mixin.js",
      "**/plugins/*Utils.js",
      "**/*.vue",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "/!node_modules/vuejs-datepicker/"
    ]
  },
  "scripts": {
    "dev-server": "encore dev-server",
    "test": "jest",
    "dev": "encore dev",
    "watch": "../utils/encore_watch.sh",
    "build": "encore production --progress"
  }
}
